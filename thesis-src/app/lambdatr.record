(defun ksi_0 (v_F tc)
  "BCS coherence length in Angstroms."
  (* 1e8 (/ (* 0.18 cgs-hbar v_F) (* cgs-k tc)))
  )

(ksi_0 1.0e8 1.54)
8889
(* 1e8 (/ (* 0.18 p_0) (* cgs-k 1.54)))
9143
(ave 8889 9143)
9016
(ksi_0 4.7e7 0.15)
4.289e+04 ;for $\rm C_8K$ 


(defun ell (I_c rho_a k_F)
  "in-plane mean-free-path in Angstroms in terms of rho_a in ohm-cm, I_c in
angstroms, and k_F in inverse cm"
    (setq rhoa (* rho_a (* (/ 1.0 9) 1e-11)))
   (/ (* 4 pi cgs-hbar I_c) (* 3 cgs-e cgs-e rhoa k_F))
   )

(ell 10.24 0.7e-6 2.75e7)
9139 ;for $\rm C_4KHg$ at 4 K
(/ (* 0.88 8889) 9139)
0.8559
(ell 5.35 0.54e-6 4.7e7)
3622 ;for $\rm C_8K$ 
